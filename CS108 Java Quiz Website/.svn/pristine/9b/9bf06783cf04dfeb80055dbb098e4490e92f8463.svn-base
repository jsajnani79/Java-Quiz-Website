<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<%@ page import="Servlets.NavBarGenerator" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Your Results</title>
<link rel="stylesheet" href="bootstrap/css/bootstrap.css"/>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://code.jquery.com/jquery.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="bootstrap/js/bootstrap.js"></script>
<style>
body {
	padding-top: 0px;
	background-color: #FFFFFF;
	overflow: auto;
	font-family: sans-serif;
}

#main { 
 margin-left:auto;
 margin-right:auto;
 width: 600px;	
 font-family: sans-serif; 
} 

h1{
text-align: center;
}
</style>
</head>
<body>

<!-- Print Complete Navigation Bar --> 
<% NavBarGenerator navBar = new NavBarGenerator();
String fullNavBar = navBar.generateFullNavBar(); %>
<%= fullNavBar %>

<div id = "main">
<div class="jumbotron">
  <h1>Quiz Results</h1>
  <br>
  <p>You have completed this quiz</p>
  <p>Score: #score</p>
  <p>Time Taken: #time</p>
  <p><a class="btn btn-primary btn-lg" role="button">Retake Quiz</a>

  <a class="btn btn-primary btn-lg pull-right" role="button">View Detailed Results</a></p>
</div>
</div> <!-- end of main div -->

</body>
</html>