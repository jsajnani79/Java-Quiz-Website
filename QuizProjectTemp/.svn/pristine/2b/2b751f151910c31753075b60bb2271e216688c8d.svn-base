<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<%@ page import="Servlets.NavBarGenerator" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Quiz Summary</title>
<link rel="stylesheet" href="bootstrap/css/bootstrap.css"/>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://code.jquery.com/jquery.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="bootstrap/js/bootstrap.js"></script>
<style>
body {
	padding-top: 0px;
	background-color: #FFFFFF;
	overflow: auto;
	font-family: sans-serif;
}

#main { 
 margin-left:auto;
 margin-right:auto;
 width: 600px;	
 font-family: sans-serif; 
} 

</style>
</head>
<body>

<!-- Print Complete Navigation Bar --> 
<% NavBarGenerator navBar = new NavBarGenerator();
String fullNavBar = navBar.generateFullNavBar(); %>
<%= fullNavBar %>

<div id="main">
<h2>Quiz Summary</h2>

<!-- Quiz Description -->
<div class="well">
<h4>Quiz Description</h4>
Information about the quiz goes here.
</div>

<!-- Quiz Creator -->
<p>This quiz was created by #creator </p>

<!-- Initiate Quiz.  -->
<button type="button" class="btn btn-default">Start Quiz</button>
<!-- Initiate Quiz in Practice Mode -->
<button type="button" class="btn btn-default pull-right">Start Practice Mode</button>
<!-- Edit Quiz, if User is Owner -->
<!-- List of the users past performance on this specific quiz.   -->
<h3>Past Performance</h3>
<div class="panel panel-default">
  <div class="panel-heading">Your Past Performance</div>
  <div class="panel-body">
  <p>You have taken this quiz a total of #timesTaken times</p>
  </div>
  <!-- Table -->
  <table class="table">
     <thead>
          <tr>
            <th>#</th>
            <th>Score</th>
            <th>Total Time</th>
            <th>Day Taken</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>1</td>
            <td>40</td>
            <td>12:01</td>
            <td>12/13/13</td>
          </tr>
          <tr>
            <td>2</td>
            <td>34</td>
            <td>11:05</td>
            <td>03/06/12</td>
          </tr>
        </tbody>
  </table>
</div>

<!-- List of the highest performers of all time.  -->
<div class="panel panel-default">
  <div class="panel-heading">All Time Highest Performers</div>

  <!-- Table -->
  <table class="table">
     <thead>
          <tr>
            <th>#</th>
            <th>User</th>
            <th>Score</th>
